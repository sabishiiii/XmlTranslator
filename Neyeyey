@Test
public void testJsonToXmlConversion_LargeArrayAtLowestLevel() {
    // JSON input with a larger array at the lowest level
    String jsonInput = "{\"name\":\"John\",\"age\":30,\"hobbies\":{\"list\":[\"reading\",\"gaming\",\"cooking\",\"traveling\",\"painting\",\"sports\",\"music\",\"photography\",\"gardening\",\"coding\"]}}";

    // Expected XML output
    String expectedXml = "<root><name>John</name><age>30</age><hobbies><list>reading</list><list>gaming</list><list>cooking</list><list>traveling</list><list>painting</list><list>sports</list><list>music</list><list>photography</list><list>gardening</list><list>coding</list></hobbies></root>";

    // Create an instance of the TranslatorServiceImpl
    TranslatorServiceImpl translatorService = new TranslatorServiceImpl();

    // Set the JSON input
    translatorService.setInputJson(jsonInput);

    // Perform the JSON to XML conversion
    translatorService.jsonToXml();

    // Read the generated XML from the Output directory
    String actualXml = readOutputFile("output.xml");

    // Assert the expected XML output
    Assertions.assertEquals(expectedXml, actualXml);
}
